{
  "name": "waterank/audit",
  "type": "yii2-extension",
  "description": "kuainiu group audit component",
  "license": "MIT",
  "authors": [
    {
      "name": "chenquan",
      "email": "sgr_cq@126.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "xlerr/task-proxy": "^1.1",
    "xlerr/yii2-common-widgets": "^1.8",
    "yiisoft/yii2": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "waterank\\audit\\": "src"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
}
